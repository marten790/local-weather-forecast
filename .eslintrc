{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "jsx-a11y/anchor-is-valid": "off",
    "object-curly-newline": ["error", { "consistent": true }],
    "linebreak-style": 0
  },
  "settings": {
    "import/resolver": {
      "webpack": { "config": "webpack.config.js" }
    }
  },
}
