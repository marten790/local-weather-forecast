{
  "name": "local-weather-forecast",
  "version": "1.0.0",
  "description": "Local weather forcast app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js, eslint **/*.jsx --fix .",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "npm run lint && npm run test:scripts",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:scripts": "jest --coverage"
  },
  "repository": "git+https://marten790@bitbucket.org/marten790/local-weather-forecast.git",
  "keywords": [
    "weather",
    "forecast"
  ],
  "author": "@marten790",
  "license": "MIT",
  "homepage": "https://bitbucket.org/marten790/local-weather-forecast#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^22.4.3",
    "jquery": "^3.4.1",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "node-sass": "^4.13.0",
    "react-addons-test-utils": "0.14.7",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-preset-stage-1": "^6.24.1",
    "classnames": "^2.2.5",
    "coveralls": "^3.0.0",
    "lodash": "3.10.1",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "next",
    "react-notify-toast": "^0.4.0",
    "react-preloadr": "^1.1.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-sparklines": "1.6.0",
    "redux": "^3.7.2",
    "redux-promise": "0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.jest"
    ],
    "moduleNameMapper": {
      "\\.scss$": "<rootDir>/.jest/style-mock.js"
    },
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "<rootDir>/.jest/shim.js",
      "<rootDir>/.jest/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.jest"
    ]
  }
}
